<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h2><%= orgName %></h2>
    <img src="<%= orgThumbnail %>">
    <div><%= orgIntroduction %></div>

    <div>管理者一覧</div>
    <% for(var i = 0; i < selectedAdminData.length; i++){ %>
      <%= selectedAdminData[i].nick_name%><%= selectedAdminData[i].name %>
      <% if(selectedAdminData[i].name === myUserName){ %>
        <input type="button" value="脱退">
      <% } %>
    <% } %>

    <div>一般メンバー一覧</div>
    <% if(typeof selectedNotAdminData !== 'undefined'){ %>
      <% for(var i = 0; i < selectedNotAdminData.length; i++){%>
        <%= selectedNotAdminData[i].nick_name %><%= selectedNotAdminData[i].name %>
        <% if (selectedNotAdminData[i].name === myUserName){ %>
          <input type="button" value="脱退">
        <% } %>
      <% } %>
    <% } %>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
  </body>
</html>
