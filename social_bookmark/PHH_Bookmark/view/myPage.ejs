<!DOCTYPE html>
<html>
<meta charset="utf-8">
<style>
#list{
  position:absolute;
  top:30px;
}

</style>
<body>
  <div id="organization"></div>
  <div id="search"><button>検索</button><input type="text"></div>
  <form accept-charset="UTF-8" action="/PHH_Bookmark/myPage" method="post">
  <div id="list">
  <input type="text" placeholder="URLを入力" id="url" name="new_bookmark_url">
  <!--<input type="text" placeholder="タイトル" id="title" name="title">
  <input type="text" placeholder="コメント" id="description" name="description">-->
  <input type="submit" value="送信">
</form>
<div id="name"></div>
<form name="my_bookmark" action="/PHH_Bookmark/myPage/delete" method="post">
<% for(var i = 0; i < list.length; i++){ %>
  <div class="main_list_item">
    <div><input type="checkbox" class="checkbox" name="id<%= bookmark_id[i] %>"><a href="<%= list[i].url %>"><%= list[i].title %></a><%= list[i].description %></div>
  </div>
<% }; %>
<button type="submit">削除</button>
</form>
<% if (typeof user !== 'undefined') { %>
      <span class="login-user"><%= user.name %>さんとしてログインしています</span>
    <% } %>
</div>
</body>
</html>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="/delete.js"></script>
